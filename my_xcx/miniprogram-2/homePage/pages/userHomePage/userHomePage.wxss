::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
button {
  border-radius: 0;
}
button::after {
  border: none;
}
page {
  height: calc(100% - 170rpx);
  background: #92D0DC;
}
.navigateContainer {
  top: 0;
  width: 100%;
  z-index: 9999;
  position: fixed;
  background: transparent;
}
.navigateContainer .navigateBar {
  display: flex;
  margin: 0 auto;
  font-size: 32rpx;
  align-items: center;
  width: calc(100% - 30rpx);
  justify-content: space-between;
  width: 100%;
}
.navigateContainer .navigateBar view {
  flex: 1;
}
.navigateContainer .navigateBar .leftIcon {
  display: flex;
  align-items: center;
}
.navigateContainer .navigateBar .leftIcon image {
  width: 24rpx;
  height: 48rpx;
  margin-left: 32rpx;
}
.userInfo {
  position: relative;
  margin-top: 334rpx;
  background: #fff;
  padding: 20rpx 40rpx 60rpx;
  border-radius: 32rpx 32rpx 0 0;
  border-bottom: 2rpx solid #F3F7F7;
}
.userInfo .userHead {
  left: 40rpx;
  top: -78rpx;
  width: 136rpx;
  height: 136rpx;
  border-radius: 50%;
  position: absolute;
  background: #fff;
  box-shadow: 0 8rpx 20rpx 0 rgba(0, 0, 0, 0.2);
}
.userInfo .btnPlane {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.userInfo .btnPlane .invite {
  font-size: 24rpx;
  color: #02BDD1;
  margin-right: 16rpx;
  border-radius: 30rpx;
  padding: 12rpx 34rpx;
  border: 2rpx solid #02BDD1;
}
.userInfo .btnPlane .follow {
  color: #fff;
  background: #02BDD1;
}
.userInfo .btnPlane .isFollow {
  color: #CACCD2;
  background: #DFF1F3;
  border: 2rpx solid #DFF1F3;
}
.userInfo .btnPlane .share {
  width: 56rpx;
  display: flex;
  height: 56rpx;
  border-radius: 50%;
  position: relative;
  align-items: center;
  background: #02BDD1;
  justify-content: center;
}
.userInfo .btnPlane .share image {
  width: 32rpx;
  height: 32rpx;
}
.userInfo .btnPlane .share .btnShare {
  top: 0;
  left: 0;
  opacity: 0;
  width: 100%;
  height: 100%;
  position: absolute;
}
.userInfo .name {
  display: flex;
  font-size: 32rpx;
  color: #2A2B2D;
  align-items: center;
  margin: 24rpx 0 16rpx;
}
.userInfo .name .title {
  overflow: hidden;
  margin-right: 20rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.userInfo .label {
  display: flex;
  flex-wrap: wrap;
  margin-top: 16rpx;
  align-items: center;
}
.userInfo .prove {
  display: flex;
  font-size: 24rpx;
  color: #62676D;
  position: relative;
  align-items: center;
  padding: 8rpx 16rpx;
  border-radius: 24rpx;
  margin: 0 20rpx 20rpx 0;
}
.userInfo .prove-bg {
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0.1;
  position: absolute;
  border-radius: 24rpx;
  background: #8CB4FF;
}
.userInfo .prove image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 6rpx;
}
.userInfo .tags {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.userInfo .tags .tag {
  font-size: 24rpx;
  color: #62676D;
  padding: 6rpx 12rpx;
  border-radius: 8rpx;
  background: #F4F6F6;
  margin: 0 16rpx 16rpx 0;
}
.userInfo .desc {
  font-size: 28rpx;
  color: #93989E;
  overflow: hidden;
  position: relative;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.userInfo .desc .btn {
  right: 0;
  bottom: 0;
  color: #508CFF;
  text-align: right;
  position: absolute;
  background: #fff;
}
.userInfo .fans {
  display: flex;
  margin-top: 40rpx;
  align-items: center;
}
.userInfo .fans-label {
  display: flex;
  font-size: 32rpx;
  color: #2A2B2D;
  align-items: center;
  margin-right: 60rpx;
}
.userInfo .fans-label view {
  font-size: 24rpx;
  color: #93989E;
  margin-left: 8rpx;
}
.listContent {
  background: #fff;
}
.listContent .null {
  display: flex;
  font-size: 28rpx;
  color: #93989E;
  margin-top: 150rpx;
  align-items: center;
  flex-direction: column;
}
.listContent .null image {
  width: 92rpx;
  height: 78rpx;
  margin-bottom: 28rpx;
}
.listContent .tab {
  display: flex;
  position: relative;
  align-items: center;
  margin: 0 auto 20rpx;
  width: calc(100% - 48rpx);
  justify-content: space-around;
}
.listContent .tab .tabs {
  font-size: 28rpx;
  padding: 24rpx 0;
  color: #93989E;
  text-align: center;
}
.listContent .tab .now {
  color: #2A2B2D;
}
.listContent .tab .line {
  bottom: 0;
  height: 6rpx;
  width: 40rpx;
  transition: all ease-in 0.2s;
  position: absolute;
  border-radius: 3rpx;
  background: #02BDD1;
}
.listContent .manuscriptList {
  height: 100%;
  padding: 0 24rpx;
  box-sizing: border-box;
}
.listContent .manuscriptList .overItem {
  opacity: 0.7;
}
.listContent .manuscriptList .manuscriptItem {
  position: relative;
  margin-bottom: 20rpx;
  border-radius: 20rpx;
  background: #F6F8F8;
  width: calc(100% - 48rpx);
  padding: 30rpx 24rpx 34rpx;
}
.listContent .manuscriptList .manuscriptItem .waterMark {
  top: 0;
  right: 0;
  font-size: 24rpx;
  color: #93989E;
  position: absolute;
  padding: 6rpx 24rpx;
  background: #E2E6E6;
  border-radius: 0 20rpx 0 20rpx;
}
.listContent .manuscriptList .manuscriptItem .title {
  font-size: 32rpx;
  color: #2A2B2D;
}
.listContent .manuscriptList .manuscriptItem .tags {
  display: flex;
  flex-wrap: wrap;
  margin: 16rpx 0 26rpx;
}
.listContent .manuscriptList .manuscriptItem .tags .tag {
  font-size: 24rpx;
  color: #62676D;
  padding: 6rpx 12rpx;
  border-radius: 8rpx;
  margin: 0 12rpx 12rpx 0;
  border: 1rpx solid #CACCD2;
}
.listContent .manuscriptList .manuscriptItem .panel {
  display: flex;
  font-size: 24rpx;
  color: #93989E;
  align-items: center;
  justify-content: space-between;
}
.listContent .manuscriptList .manuscriptItem .panel .amount {
  font-size: 28rpx;
  color: #ED510B;
}
.listContent .evaluateContent .scorePanel {
  display: flex;
  font-size: 24rpx;
  color: #93989E;
  margin-top: 26rpx;
  align-items: center;
  justify-content: space-between;
}
.listContent .evaluateContent .scorePanel .label {
  flex: 1;
  display: flex;
  align-items: center;
}
.listContent .evaluateContent .scorePanel .label text {
  color: #62676D;
  margin-left: 8rpx;
}
.mask {
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  position: fixed;
  flex-direction: column;
  display: flex;
  align-items: inherit;
  justify-content: flex-end;
  background: rgba(0, 0, 0, 0.5);
}
.mask .content {
  max-height: 70%;
  background: #fff;
  box-sizing: border-box;
  flex-direction: column;
  padding: 12rpx 40rpx 40rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  border-radius: 24rpx 24rpx 0 0;
}
.mask .content .bar {
  height: 8rpx;
  width: 120rpx;
  border-radius: 100rpx;
  background: #EEEEEE;
}
.mask .content .title {
  width: 100%;
  font-size: 32rpx;
  color: #2A2B2D;
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.mask .content .title navigator {
  font-size: 28rpx;
  color: #02bdd1;
}
.mask .content .null {
  text-align: center;
  margin: 140rpx auto;
}
.mask .content .null image {
  width: 68rpx;
  height: 78rpx;
  margin: 40rpx auto 20rpx;
}
.mask .content .null .explain {
  font-size: 28rpx;
  color: #93989E;
}
.mask .content .list {
  flex: 1;
  width: 100%;
  overflow: auto;
}
.mask .content .list .listItem {
  display: flex;
  margin-top: 40rpx;
  align-items: center;
}
.mask .content .list .listItem .cover {
  width: 88rpx;
  height: 88rpx;
  flex-shrink: 0;
  margin-right: 24rpx;
}
.mask .content .list .listItem .info {
  flex: 1;
}
.mask .content .list .listItem .info .top {
  display: flex;
  font-size: 28rpx;
  color: #2A2B2D;
  margin-bottom: 8rpx;
  justify-content: space-between;
}
.mask .content .list .listItem .info .top .name {
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.mask .content .list .listItem .info .top .amount {
  flex-shrink: 0;
  color: #ED510B;
}
.mask .content .list .listItem .info .bottom {
  display: flex;
  font-size: 24rpx;
  color: #93989E;
  align-items: center;
}
.mask .content .list .listItem .info .bottom view:nth-child(1) {
  margin-right: 20rpx;
}
.mask .content .list .listItem .info .bottom image {
  width: 20rpx;
  height: 20rpx;
  margin-right: 8rpx;
}
.hint {
  top: 0;
  width: 100%;
  height: 100%;
  display: flex;
  position: fixed;
  align-items: center;
  justify-content: center;
}
.hint .text {
  font-size: 28rpx;
  color: #fff;
  line-height: 21px;
  padding: 15rpx 66rpx;
  background: rgba(0, 0, 0, 0.75);
  border-radius: 18px;
}
.layer-backdrop {
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  overflow: hidden;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 9999;
  -webkit-tap-highlight-color: transparent;
}
.layer-backdrop .modal {
  background: #fff;
  border-radius: 12px;
  width: 85%;
}
.layer-backdrop .modal .modal-body {
  width: 100%;
  border-radius: 12px;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.layer-backdrop .modal .modal-body .title {
  font-size: 26rpx;
  color: #62676D;
  letter-spacing: 0;
  line-height: 22px;
  margin-top: 64rpx;
}
.layer-backdrop .modal .modal-body .content {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 34rpx;
  color: #353535;
  width: 100%;
  padding: 0 24px;
  padding: 20rpx 48rpx 64rpx 48rpx;
  border-bottom: 0.5px solid #F3F3F3;
  box-sizing: border-box;
  text-align: center;
  letter-spacing: -0.22px;
  font-weight: bold;
}
.layer-backdrop .modal .modal-body .btn {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 56px;
  font-size: 17px;
  color: #576B95;
  font-weight: bold;
  width: 100%;
}
.layer-backdrop .modal .modal-body .cancel {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 17px;
  color: #000;
  border-right: 0.5px solid #F3F3F3;
  font-weight: bold;
}
.actionSheet {
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  z-index: 10000;
  position: fixed;
  flex-direction: column;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  background: rgba(0, 0, 0, 0.5);
}
.actionSheet .dialog {
  width: 100%;
  color: #000;
  font-size: 34rpx;
  background: #f2f2f2;
  border-radius: 24rpx 24rpx 0 0;
}
.actionSheet .dialog-item {
  padding: 32rpx 0;
  text-align: center;
  line-height: unset;
  background: #fff;
  border-bottom: 1rpx solid #f2f2f2;
}
.actionSheet .dialog-item:first-child {
  border-radius: 24rpx 24rpx 0 0;
}
.actionSheet .dialog-cancel {
  border: 0;
  margin-top: 16rpx;
}
