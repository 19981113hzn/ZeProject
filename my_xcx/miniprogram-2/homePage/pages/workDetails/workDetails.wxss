page {
  width: 100%;
  height: 100%;
  background: #121216;
}
#box {
  height: 100%;
}
.navigateContainer {
  top: 0;
  width: 100%;
  height: 15vh;
  z-index: 9999;
  position: fixed;
  background-image: linear-gradient(0deg, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.5) 100%);
}
.navigateContainer .navigateBar {
  display: flex;
  margin: 0 auto;
  font-size: 34rpx;
  align-items: flex-end;
  width: calc(100% - 30rpx);
  justify-content: space-between;
}
.navigateContainer .navigateBar view {
  flex: 1;
}
.navigateContainer .navigateBar .leftIcon .icon {
  height: 100%;
  display: inline-flex;
  align-items: center;
  border-radius: 81rpx;
  border: 1rpx solid rgba(255, 255, 255, 0.2);
}
.navigateContainer .navigateBar .leftIcon .icon image {
  width: 18rpx;
  height: 36rpx;
  padding: 14rpx 36rpx;
}
.navigateContainer .navigateBar .leftIcon .icon image:last-child {
  width: 40rpx;
  height: 38rpx;
  position: relative;
}
.navigateContainer .navigateBar .leftIcon .icon image:last-child::before {
  left: 0;
  content: '';
  width: 2rpx;
  height: 37rpx;
  position: absolute;
  background: rgba(255, 255, 255, 0.2);
}
.navigateContainer .navigateBar .title {
  height: 100%;
  opacity: 0.9;
  color: #fff;
  text-align: center;
}
.showWorks {
  width: 100%;
  height: 100%;
}
.showWorks swiper-item {
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
}
.showWorks swiper-item image {
  width: 100%;
  height: 100%;
}
.showWorks swiper-item .errorImg {
  width: 100%;
  display: flex;
  padding: 300rpx 0;
  align-items: center;
  background: #f4f6f8;
  justify-content: center;
}
.showWorks swiper-item .errorImg image {
  width: 92rpx;
  height: 78rpx;
}
.panel {
  bottom: 0;
  position: fixed;
  width: calc(100% - 64rpx);
  padding: 46rpx 32rpx 80rpx;
  background-image: linear-gradient(-180deg, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.8) 100%);
}
.panel .info {
  display: flex;
  align-items: center;
  flex: 1;
}
.panel .info .headImg {
  width: 64rpx;
  height: 64rpx;
  flex-shrink: 0;
  border-radius: 50%;
  margin-right: 20rpx;
}
.panel .info .name {
  color: #fff;
  font-size: 40rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.panel .info .arrow {
  width: 16rpx;
  height: 30rpx;
  margin-left: 24rpx;
}
.panel .likeImg {
  width: 104px;
  height: 32px;
  margin-right: -16px;
}
.panel .tag {
  display: flex;
  overflow: auto;
  align-items: center;
  margin: 20rpx auto 0;
}
.panel .tag .tags {
  font-size: 24rpx;
  color: #62676D;
  white-space: nowrap;
  border-radius: 8rpx;
  padding: 6rpx 24rpx;
  background: #DFF3F3;
  margin: 0 16rpx 16rpx 0;
}
.panel .desc {
  color: #fff;
  min-height: 1rpx;
  font-size: 24rpx;
  margin: 0 auto 24rpx;
  overflow: auto;
  height: 66rpx;
}
.panel .illegal {
  display: flex;
  align-items: center;
}
.panel .illegal .image {
  width: 32rpx;
  height: 32rpx;
}
.panel .illegal .text {
  font-size: 28rpx;
  color: #ED510B;
  margin-left: 12rpx;
}
.panel .clickIcon {
  display: flex;
  margin-top: 24rpx;
  position: relative;
  align-items: center;
  justify-content: space-between;
}
.panel .clickIcon .left {
  display: flex;
  color: #fff;
  font-size: 24rpx;
  align-items: center;
  filter: opacity(0.4);
}
.panel .clickIcon .left .delete {
  width: 36rpx;
  height: 38rpx;
  margin-right: 20rpx;
}
.panel .clickIcon .left .edit {
  width: 32rpx;
  height: 34rpx;
  margin-left: 20rpx;
}
.panel .clickIcon .right {
  display: flex;
  align-items: center;
}
.panel .clickIcon .right .tipsPopup {
  left: 0;
  width: 91%;
  top: -220rpx;
  padding: 30rpx;
  font-size: 24rpx;
  color: #3F3F3F;
  position: absolute;
  background: #fff;
  border-radius: 16rpx;
  box-shadow: 0 2px 8px 0 rgba(0, 0, 0, 0.15);
}
.panel .clickIcon .right .tipsPopup::after {
  width: 0;
  height: 0;
  content: '';
  right: 106rpx;
  bottom: -10rpx;
  position: absolute;
  border-top: 24rpx solid #fff;
  border-left: 24rpx solid transparent;
  border-right: 24rpx solid transparent;
}
.panel .clickIcon .right .question {
  width: 34rpx;
  height: 34rpx;
  margin-right: 40rpx;
}
.panel .clickIcon .right .like {
  display: flex;
  color: #fff;
  font-size: 30rpx;
  align-items: center;
}
.panel .clickIcon .right .like image {
  width: 40rpx;
  height: 34rpx;
  margin-right: 16rpx;
  filter: opacity(0.4);
}
