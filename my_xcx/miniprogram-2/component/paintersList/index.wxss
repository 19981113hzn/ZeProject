.list {
  padding: 0 24rpx;
  width: calc(100% - 48rpx);
}
.list .listItem {
  position: relative;
  width: 100%;
  margin-top: 24rpx;
  border-radius: 0 0 32rpx 32rpx;
  overflow: hidden;
}
.list .listItem .panel {
  display: flex;
  background: #fff;
  align-items: center;
  padding: 10rpx 24rpx;
  justify-content: space-between;
  border-radius: 32rpx 32rpx 0 0;
}
.list .listItem .panel .left {
  display: flex;
  overflow: hidden;
  align-items: center;
}
.list .listItem .panel .left .headImg {
  width: 68rpx;
  height: 68rpx;
  flex-shrink: 0;
  border-radius: 50%;
  margin-right: 20rpx;
}
.list .listItem .panel .left .info {
  flex: 1;
  font-size: 28rpx;
  overflow: hidden;
  color: #2A2B2D;
}
.list .listItem .panel .left .info .tags {
  font-size: 24rpx;
  color: #93989E;
  line-height: 34rpx;
  white-space: nowrap;
}
.list .listItem .panel .invite {
  color: #fff;
  font-size: 24rpx;
  padding: 8rpx 30rpx;
  border-radius: 24rpx;
  background: #02BDD1;
}
.list .listItem .panel .hasInvite {
  display: flex;
  flex-shrink: 0;
  font-size: 22rpx;
  color: #62676D;
  align-items: center;
}
.list .listItem .panel .hasInvite .icon {
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}
.list .listItem .coverBox {
  width: 100%;
  height: 376rpx;
  overflow: hidden;
  border-radius: 0 0 32rpx 32rpx;
  background: #F3F7F7;
}
.list .listItem .coverBox .swiper {
  width: 100%;
  height: 376rpx;
  border-radius: 0 0 32rpx 32rpx;
  background: #F3F7F7;
  overflow: hidden;
}
.list .listItem .coverBox .swiper .cover {
  width: 100%;
  height: 100%;
  border-radius: 0 0 32rpx 32rpx;
  background: #F3F7F7;
  overflow: hidden;
}
.list .listItem .bottom {
  bottom: 0;
  display: flex;
  color: #fff;
  font-size: 24rpx;
  position: absolute;
  width: 100%;
  justify-content: space-between;
  background: linear-gradient(-180deg, rgba(0, 0, 0, 0) 3%, rgba(0, 0, 0, 0.5) 100%);
  border-radius: 0 0 32rpx 32rpx;
  overflow: hidden;
  transform: tanslate3d(0, 0, 0);
  transform: translate(0%, 0px) translateZ(0px);
  z-index: 21;
}
.list .listItem .bottom .like {
  display: flex;
  align-items: center;
  padding: 20rpx 28rpx;
}
.list .listItem .bottom .like .likeIcon {
  width: 40rpx;
  height: 34rpx;
  margin-right: 8rpx;
}
.list .listItem .bottom .page {
  padding: 20rpx 28rpx;
}
.bottomLoading {
  width: 68rpx;
  height: 12rpx;
  display: block;
  margin: 0px auto;
  padding: 20rpx 0;
}
